    "@types/jest": "^29.5.12",
    "@types/moxios": "^0.4.17",
    "@types/node": "^22.1.0",
    "jest": "^29.7.0",
    "moxios": "^0.4.0",
    "rimraf": "^6.0.1",
    "ts-jest": "^29.2.4",
    "typedoc": "^0.26.5",
    "typescript": "^5.5.4",
    "vmix-js-utils": "^4.0.16"
